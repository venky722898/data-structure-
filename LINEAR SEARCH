#include <stdio.h>

int main()
{
    int n,arr[100],ele,i;
	printf("Enter the no of elements:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
	    printf("Enter the elements:");
		scanf("%d",&arr[i]);
	}
	printf("Enter the element to search:");
	scanf("%d",&ele);
	for(i=0;i<n;i++)
	{
		if (arr[i]==ele)
		{
			printf(" %d is found at the position %d",ele,i+1);
		}
		
	}

}
